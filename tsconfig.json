{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES2018",
    "strict": true,
    "module": "ES2015",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "noFallthroughCasesInSwitch": true,
    "types": ["jest", "node", "@typescript-eslint/utils"]
  }
}
