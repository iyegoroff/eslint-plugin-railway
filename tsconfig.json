{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES2022",
    "strict": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "noFallthroughCasesInSwitch": true,
    "types": [
      "jest",
      "node",
      "@typescript-eslint/utils"
    ]
  }
}